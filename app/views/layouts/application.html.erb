<!DOCTYPE html>
<html>
  <head>
    <title>Sobareco</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
　　　

<div class="container-fluid">
  <div class="row">
    <div class="col-lg-12" style="background-color: ghostwhite";>
      <div id= "slidemenu">
        <div class="col-md-2 col-md-offset-0">
            <h2>sobareco'</h2>
        </div>
          <div class="col-md-1 col-md-offset-5">
               <%= link_to "ログイン", new_admin_admin_user_session_path %>
          </div>
          <div class="col-md-1 col-md-offset-1">
               <%= link_to "ログアウト",  destroy_admin_admin_user_session_path, method: :delete %>
          </div>
          <div class="col-md-1 col-md-offset-1">
              <p>検索</p>
          </div>
      </div>
    </div>
  </div>
</div>



<div class="container-fluid">
  <div class="row">
    <div class="col-lg-2">
        <table class="table table-bordered">
          <tbody>
            <tr>
              <td>商品一覧</td>
            </tr>
            <tr>
              <td>商品登録</td>
            </tr>
            <tr>
               <td><%= link_to '入荷一覧', admin_arrival_managements_path %></td>
            </tr>
            <tr>
              <td><%= link_to '入荷登録', new_admin_arrival_management_path %></td>
            </tr>
            <tr>
              <td><%= link_to '注文一覧',  admin_orders_path %></td>
            </tr>
            <tr>
              <td><%= link_to 'アーティスト一覧', admin_artists_path %></td>
            </tr>
            <tr>
              <td><%= link_to 'レーベル登録', admin_labels_path %></td>
            </tr>
            <tr>
              <td><%= link_to 'ジャンル登録', admin_genres_path %></td>
            </tr>
          </tbody>
        </table>
   </div>

    <%= yield %>
  </body>
</html>


