<h1>入荷登録一覧</h1>


<div class="col-lg-8">
	<table class="table table-hover">
		<thead>
			<tr>
				<th>商品名</th>
				<th>アーティスト名</th>
				<th>入荷日時</th>
				<th>入荷個数</th>
			</tr>
		</thead>
	<tbody>
     <% @arrival.each do |arrival| %>
			<tr>
				<td><%= arrival.product_id %></td>
				<td>アーティスト名</td>
				<td><%= arrival.arrival_date %></td>
				<td><%= arrival.arrival_count %></td>
　　　　     </tr>
<% end %>
<%= paginate @arrival %>




